{
    "root": true,
    "extends": "next",
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ],
        "react/no-unknown-property": [
            2,
            {
              "ignore": ["jsx", "global"]
            }
          ],
        "react/display-name": 0
    }
}